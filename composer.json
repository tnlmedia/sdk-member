{
    "name": "tnlmedia/member-sdk",
    "description": "Member PHP SDK",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "TNLMediagene",
            "email": "to.it@inkmaginecms.com"
        }
    ],
    "require": {
        "php": "~7.1 || ^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "TNLMedia\\MemberSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TNLMedia\\MemberSDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
